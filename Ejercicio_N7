using System;

class Program {
    static void Main() {
        Console.Write("Ingrese el n√∫mero para la tabla: ");
        int numero = Convert.ToInt32(Console.ReadLine());

        Console.WriteLine($"\nTabla de multiplicar del {numero}:");
        for (int i = 1; i <= 12; i++) {
            Console.WriteLine($"{numero} x {i} = {numero * i}");
        }
    }
}
